{
    "printWidth": 100,
    "tabWidth": 4,
    "semi": false,
    "singleQuote": false,
    "quoteProps": "as-needed",
    "trailingComma": "es5",
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "htmlWhitespaceSensitivity": "ignore",
    "vueIndentScriptAndStyle": false,
    "endOfLine": "auto",
    "prettier.eslintIntegration": true,
    "singleAttributePerLine": false
}
